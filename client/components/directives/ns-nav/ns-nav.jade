#navStyle.navbar.navbar-default(role='navigation')
  .container
    .navbar-header
      button.navbar-toggle(data-toggle='collapse', data-target='#bs-navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href='/') D&amp;D Online

    .collapse.navbar-collapse#bs-navbar-collapse
      ul.nav.navbar-nav.navbar-right(ng-hide='email')
        li(ng-show='connected'): a(href='#') ONLINE
        li: a(ui-sref='home') Quest
        li: a(ui-sref='register') Register
        li: a(ui-sref='login') Login
      ul.nav.navbar-nav.navbar-right(ng-show='email')
        li(ng-show='connected'): a(href='#') ONLINE
        li: a(ui-sref='home') Quest
        li: a(ui-sref='rooms') Dungeons
        li: a(ui-sref='charCreate') Heros
        li: a(ui-sref='logout') {{email}}
